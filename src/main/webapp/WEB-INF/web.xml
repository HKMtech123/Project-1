<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
        <servlet-name>insert</servlet-name>
        <servlet-class>com.servlet.SaveDetails</servlet-class>
    </servlet>
  <servlet>
  	<servlet-name>Insert_Details</servlet-name>  	
  	<servlet-class>com.servlet.Insert_Details</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteDetails</servlet-name>
  	<display-name>DeleteDetails</display-name>
  	<description></description>
  	<servlet-class>com.servlet.DeleteDetails</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditDetails</servlet-name>
  	<display-name>EditDetails</display-name>
  	<description></description>
  	<servlet-class>com.servlet.EditDetails</servlet-class>
  </servlet>
    <servlet-mapping>
        <servlet-name>insert</servlet-name>
        <url-pattern>/SaveDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>Insert_Details</servlet-name>
    	<url-pattern>/Insert_Details</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>DeleteDetails</servlet-name>
    	<url-pattern>/DeleteDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>EditDetails</servlet-name>
    	<url-pattern>/EditDetails</url-pattern>
    </servlet-mapping>
  
</web-app>
